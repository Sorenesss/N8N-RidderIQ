{
    "name": "n8n-nodes-ridderiq",
    "version": "1.0.12",
    "description": "RidderIQ API integration for n8n",
    "license": "MIT",
    "homepage": "https://github.com/Sorenesss/n8n-ridderiq",
    "keywords": [
        "n8n",
        "n8n-community-node",
        "ridderiq",
        "api",
        "n8n-community-node-package"
    ],
    "author": {
        "name": "soreness",
        "email": "reinoutvg18@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Sorenesss/n8n-ridderiq.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "dev:manual": "npm run build && npm link && N8N_CUSTOM_EXTENSIONS=$(pwd) n8n start",
        "link": "npm link",
        "unlink": "npm unlink",
        "format": "prettier nodes credentials --write",
        "lint": "n8n-node lint",
        "lintfix": "n8n-node lint --fix",
        "test": "n8n-node test",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "main": "index.js",
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/RidderIQApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/RidderIq/RidderIq.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "^0.13.0",
        "@types/markdown-it": "^14.1.2",
        "@types/node": "^22.7.5",
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-n8n-nodes-base": "^1.16.3",
        "gulp": "^5.0.1",
        "gulp-cli": "^3.1.0",
        "n8n-workflow": "*",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
